unit Principal;

interface

uses
  System.SysUtils, System.Types, System.UITypes, System.Classes, System.Variants,
  FMX.Types, FMX.Controls, FMX.Forms, FMX.Graphics, FMX.Dialogs,
  FMX.Controls.Presentation, FMX.StdCtrls, FMX.Layouts,Usuarios, FMX.Objects;

type
  TForm2 = class(TForm)
    ScaledLayout1: TScaledLayout;
    Contenedor: TRectangle;
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
  Frame_Usuario : TFrame1;
    { Public declarations }
  end;

var
  Form2: TForm2;

implementation

{$R *.fmx}

procedure TForm2.FormCreate(Sender: TObject);
begin
    //Crear
    Frame_Usuario := TFrame1.Create(nil);
    Frame_Usuario.Parent:=Contenedor;
    Frame_Usuario.Visible:=True;
end;

end.
